<Window x:Class="VehicleRentalSystem_V1._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VehicleRentalSystem_V1._0"
        mc:Ignorable="d"
        Title="CarRent" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <DockPanel LastChildFill="False" Background="#FF823A3A">
        <DockPanel LastChildFill="False" Background="#FFA7C2C3" Width="200">
            <Button Content="Button" DockPanel.Dock="Top" Height="50" BorderBrush="{x:Null}"/>
            <Button Content="Button" DockPanel.Dock="Top" BorderBrush="{x:Null}" Height="50"/>
        </DockPanel>
    </DockPanel>
</Window>
